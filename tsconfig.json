{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "dist",
    "strict": true,
    "moduleDetection": "force",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "react-router-dom": ["node_modules/react-router-dom"]
    }
  },
  "ts-node": {
    "esm": true
  },
  "include": ["src"]